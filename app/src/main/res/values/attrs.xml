<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="follow_seekbar">
        <attr name="fs_margin_left" format="dimension" /> <!-- 进度条距离左侧按钮距离 -->
        <attr name="fs_margin_right" format="dimension" /> <!-- 进度条距离右侧按钮距离 -->
        <attr name="fs_text_size" format="dimension" /> <!-- 字体大小 -->
        <attr name="fs_text_color" format="color" /> <!-- 字体颜色 -->
        <attr name="fs_progressDrawable" format="reference" /> <!-- SeekBar的背景图片 -->
        <attr name="fs_thumb" format="reference" /> <!-- 表示SeekBar的滑块图片 -->
    </declare-styleable>

    <declare-styleable name="VerticalSeekBar">
        <attr name="vertical_color" format="color" />
        <attr name="circle_radius" format="dimension" />
        <attr name="circle_color" format="color" />
        <attr name="dragable" format="boolean" />
        <attr name="image_background" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomVerticalSeekBar">
        <attr name="vertical_background_color" format="color" /> <!-- 进度背景颜色 -->
        <attr name="vertical_progress_background_color" format="color" /> <!-- 有效进度背景颜色 -->
        <attr name="vertical_circle_radius" format="dimension" />
        <attr name="vertical_circle_color" format="color" />
        <attr name="vertical_dragable" format="boolean" />
        <attr name="vertical_image_background" format="reference" />
        <attr name="vertical_background_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SeekBarVertical">
        <attr name="UISeekBarProgress" format="integer" />
        <attr name="UISeekBarMax" format="integer" />
        <attr name="UISeekBarIcon" format="reference" />
        <attr name="UISeekBarProgressDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="Fgki8ylkCustomVerticalSeekBar">
        <attr name="ddkkyuuy_vertical_circle_radius" format="dimension" />
        <attr name="ddkkyuuy_vertical_background_color" format="color|reference" /> <!-- 进度背景颜色 -->
        <attr name="ddkkyuuy_vertical_progress_background_color" format="color|reference" /> <!-- 有效进度背景颜色 -->
        <attr name="ddkkyuuy_vertical_background_width" format="dimension" />
        <attr name="ddkkyuuy_vertical_circle_color" format="color" />
        <attr name="ddkkyuuy_vertical_dragable" format="boolean" />
        <attr name="ddkkyuuy_vertical_image_background" format="reference" />
        <attr name="ddkkyuuy_vertical_background" format="reference" />
        <attr name="ddkkyuuy_vertical_start_color" format="color" />
        <attr name="ddkkyuuy_vertical_end_color" format="color" />
    </declare-styleable>

</resources>