<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seekbardemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".follow.FollowActivity" />
        <activity android:name=".change.ChangeActivity" />
        <activity android:name=".change.ChangeActivity2" />
        <activity android:name=".vertical_seekbar.VerticalSeekbarActivity" />
        <activity android:name=".vertical_seekbar2.VerticalSeekbarActivity2" />
        <activity android:name=".vertical_seekbar3.VerticalSeekbarActivity3" />
        <activity android:name=".vertical_seekbar4.VerticalSeekbarActivity4" />
        <activity android:name=".vertical_seekbar5.VerticalSeekbarActivity5" />


    </application>

</manifest>